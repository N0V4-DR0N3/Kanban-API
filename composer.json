{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [ "laravel", "framework" ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ext-bcmath": "*",
        "ext-gettext": "*",
        "ext-exif": "*",
        "ext-mbstring": "*",
        "ext-redis": "*",
        "cweagans/composer-patches": "^1.7",
        "laravel/framework": "^11.0",
        "laravel/horizon": "*",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.9",
        "league/oauth2-client": "^2.7",
        "saloonphp/laravel-http-sender": "^3.0",
        "saloonphp/laravel-plugin": "^3.5",
        "saloonphp/saloon": "^3.8",
        "spatie/laravel-data": "^4.5",
        "spatie/laravel-permission": "^6.4",
        "symplify/vendor-patches": "^11.3"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "lucascudo/laravel-pt-br-localization": "^2.1",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^11.0",
        "spatie/laravel-ignition": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "patches": {
            "spatie/laravel-data": [
                "patches/spatie-laravel-data-src-optional-php.patch",
                "patches/spatie-laravel-data-src-support-factories-datatypefactory-php.patch"
            ]
        },
        "laravel": {
            "dont-discover": [ ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "cweagans/composer-patches": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
